generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model user {
  id           Int     @id @default(autoincrement()) @db.UnsignedInt
  email        String  @db.VarChar(255)
  display_name String  @db.VarChar(255)
}
